{
  "name": "Sprite and script count",
  "description": "Shows the number of sprites and scripts a project has next to the Remix button.",
  "tags": ["community", "projectPage"],
  "credits": [
    {
      "name": "TheColaber",
      "link": "https://scratch.mit.edu/users/TheColaber"
    }
  ],
  "userscripts": [
    {
      "url": "projectstats.js",
      "matches": ["projects"]
    },
    {
      "url": "blockcount.js",
      "matches": ["projects"],
      "settingMatch": {
        "id": "editorCount",
        "value": true
      }
    }
  ],
  "userstyles": [
    {
      "url": "userscript.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "id": "editorCount",
      "name": "Live block count in editor",
      "type": "boolean",
      "default": false
    }
  ],
  "versionAdded": "1.4.0"
}
